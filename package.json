{
  "name": "javascript-development-environment",
  "version": "1.0.0",
  "description": "JavaScript development environment Pluralsight course by Cory House",
  "scripts": {
    "prestart": "babel-node buildScripts/startMsg.js",
    "start": "npm-run-all --parallel echo-test src:open lint:watch test:watch",
    "src:open": "babel-node buildScripts/srcServer.js",
    "echo-test": "node -p \"console.log('test')\"",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --report progress --require babel-register buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Giorgi Ghughunishvili",
  "license": "MIT",
  "dependencies": {
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.4",
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "^1.7.4",
    "cross-env": "3.1.3",
    "css-loader": "^3.6.0",
    "eslint": "4.18.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-watch": "^7.0.0",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "^4.3.0",
    "jsdom": "9.8.0",
    "json-schema-faker": "^0.4.7",
    "json-server": "^0.16.1",
    "localtunnel": "^2.0.0",
    "mocha": "^8.0.1",
    "nock": "^13.0.2",
    "npm-run-all": "3.1.1",
    "numeral": "1.5.3",
    "open": "~6.0.0",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "surge": "^0.21.6",
    "webpack": "^4.43.0",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5"
  }
}
